<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context  
	http://www.springframework.org/schema/context/spring-context-2.5.xsd"
	default-autowire="byName">

	<context:component-scan base-package="com.fzjt" />
	<context:annotation-config />
	<bean id="helpCenterDAO"
		class="com.fzjt.xiaoliu.read.interfaces.business.helpcenter.dao.impl.HelpCenterDAOImpl">
		<property name="dataSource" ref="dataSource" />
		<property name="sqlMapClient" ref="sqlMapClient" />
	</bean>
	<!--注入DAO  -->
	<bean id="helpCenterService"
		class="com.fzjt.xiaoliu.read.interfaces.business.helpcenter.service.impl.HelpCenterServiceImpl">
		<property name="helpCenterDAO" ref="helpCenterDAO" />
	</bean>
</beans>